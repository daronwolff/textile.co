<% if clocks.size > 0%>
<table class="bordered ">
  <thead>
    <tr>
      <th>Employee</th>
      <th>Date</th>
      <th>Evaluation</th>
      <th>Moment</th>
     </tr>
  </thead>
  <tbody>
    <% clocks.each do |clock| %>
    <tr>
      <td>
        <%= link_to clock.employee.first_name+" "+clock.employee.last_name, clock.employee %>
      </td>
      <td><%= clock.date.strftime("%Y-%m-%d %H:%M:%S") %></td>
      <td><%= @evaluation[clock.evaluation.to_i] %></td>
      <td><%= clock.moment.name %></td>
     </tr>
    <% end %>
  </tbody>
</table>
<% else %>
    <div class="row">
      <%= render "/partials/no_data",path: false, section_name: "Employees list" %>
    </div>
<% end %>
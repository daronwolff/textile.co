<p id="notice"><%= notice %></p>

<h1>Listing Clocks</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Employee</th>
      <th>Evaluation</th>
      <th>Moment</th>
      <th>Diference</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clocks.each do |clock| %>
      <tr>
        <td><%= clock.date %></td>
        <td><%= clock.employee %></td>
        <td><%= clock.evaluation %></td>
        <td><%= clock.moment %></td>
        <td><%= clock.diference %></td>
        <td><%= link_to 'Show', clock %></td>
        <td><%= link_to 'Edit', edit_clock_path(clock) %></td>
        <td><%= link_to 'Destroy', clock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clock', new_clock_path %>
